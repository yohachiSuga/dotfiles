{
    "window.zoomLevel": 2,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "hediet.vscode-drawio.local-storage": "",
    "hediet.vscode-drawio.offline": false,
    "vim.insertModeKeyBindings":[
    	{
		"before":["j","j"],
		"after":["<Esc>"]
	}
    ],
    "vim.hlsearch": true,
	"vim.easymotion":true,
	"vim.easymotionKeys":"sdfghjkl",
	"vim.leader":"<space>",
	"vim.useSystemClipboard":true,
    "vim.handleKeys":{
        // use gh instead of <C-k>
        "<C-k>": false,
        // use ctrl+p for go to file
        "<C-p>": false,
        // use copilot chat
        "<C-i>": false
    },
    "editor.tabSize":4,
    "editor.insertSpaces":true,
    "editor.detectIndentation": false,
    "terminal.integrated.commandsToSkipShell": [
        "-workbench.action.terminal.findNext"
    ],
    "editor.formatOnSave": true,
    "clang-format.style": "{ BasedOnStyle: LLVM, IndentWidth: 4, ColumnLimit: 160 }",
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    }
}
