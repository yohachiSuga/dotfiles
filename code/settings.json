{
    "window.zoomLevel": 5,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "hediet.vscode-drawio.local-storage": "",
    "hediet.vscode-drawio.offline": false,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.hlsearch": true,
    "vim.easymotion": true,
    "vim.easymotionKeys": "sdfghjkl",
    "vim.leader": "<space>",
    "vim.useSystemClipboard": true,
    "vim.handleKeys": {
        // use gh instead of <C-k>
        "<C-k>": false,
        // use ctrl+p for go to file
        "<C-p>": false,
        // use copilot chat
        "<C-i>": false
    },
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "terminal.integrated.commandsToSkipShell": [
        "-workbench.action.terminal.findNext"
    ],
    "editor.formatOnSave": true,
    "clang-format.style": "{ BasedOnStyle: LLVM, IndentWidth: 4, ColumnLimit: 160 }",
    "[cpp]": {
        "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
    },
    "C_Cpp.intelliSenseEngine": "disabled",
    "clangd.path": "/home/akira/.config/Code/User/globalStorage/llvm-vs-code-extensions.vscode-clangd/install/15.0.6/clangd_15.0.6/bin/clangd",
    "redhat.telemetry.enabled": false,
    "[python]": {
        "editor.formatOnType": true
    },
    "workbench.colorTheme": "Default Dark+",
    "vsnotes.defaultNotePath": "/home/akira/workspace/notes",
    "[rust]": {
        "editor.formatOnSave": true
    },
    "rust-analyzer.checkOnSave": true,
    "rust-analyzer.check.command": "clippy",
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.panel.defaultLocation": "right"
}